{
  "name": "forlond/phpunit-testing-tools",
  "type": "library",
  "description": "A set of utilities for PHPUnit testing",
  "keywords": [
    "PhpUnit",
    "testing",
    "tools"
  ],
  "homepage": "https://github.com/forlond/phpunit-testing-tools",
  "license": "MIT",
  "authors": [
    {
      "name": "Carlos DomÃ­nguez",
      "email": "ixarlie@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "phpunit/phpunit": "^9.5 || ^10"
  },
  "require-dev": {
    "psr/log": "2.0.x-dev || 3.x-dev",
    "jms/serializer": "3.x-dev",
    "symfony/expression-language": "5.4.x-dev || 6.4.x-dev"
  },
  "conflict": {
    "psr/log": "<2.0",
    "jms/serializer": "<3.0",
    "symfony/expression-language": "<5.4.0"
  },
  "autoload": {
    "psr-4": {
      "Forlond\\TestTools\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Forlond\\TestTools\\Tests\\": "tests/"
    },
    "files": [
    ]
  }
}
