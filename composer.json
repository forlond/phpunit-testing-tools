{
  "name": "forlond/phpunit-testing-tools",
  "type": "library",
  "description": "A set of utilities for PHPUnit testing",
  "keywords": [
    "PhpUnit",
    "testing",
    "tools"
  ],
  "homepage": "https://github.com/forlond/phpunit-testing-tools",
  "license": "MIT",
  "authors": [
    {
      "name": "Carlos DomÃ­nguez",
      "email": "ixarlie@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "phpunit/phpunit": "^10 || ^11 || ^12"
  },
  "require-dev": {
    "doctrine/orm": "^2.0",
    "jms/serializer": "^3.0",
    "psr/log": "^1.0 || ^2.0 || ^3.0",
    "symfony/event-dispatcher": "^6.4 || ^7.3",
    "symfony/expression-language": "^6.4 || ^7.3",
    "symfony/form": "^6.4 || ^7.3",
    "symfony/http-client": "^6.4 || ^7.3",
    "symfony/mailer": "^6.4 || ^7.3",
    "symfony/notifier": "^6.4 || ^7.3",
    "symfony/security-http": "^6.4 || ^7.3",
    "symfony/translation": "^6.4 || ^7.3",
    "symfony/validator": "^6.4 || ^7.3",
    "symfony/workflow": "^6.4 || ^7.3"
  },
  "config": {
    "sort-packages": true
  },
  "conflict": {
  },
  "autoload": {
    "psr-4": {
      "Forlond\\TestTools\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Forlond\\TestTools\\Tests\\": "tests/"
    },
    "files": [
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}
